userName = 'Darek'
userID = 'dktomaszenko'
def mostrarVersionMaven(String a)
{
    bat "java -version"
    echo a
}

pipeline {
    agent any

    stages {
        stage('Nombre usuario') {
            steps {
                println userName
            }
        }
        stage('ID usuario') {
            steps {
                println userID
            }
        }
        stage('Java version') {
            steps {
                mostrarVersionMaven()
            }
        }
     
    }
}
