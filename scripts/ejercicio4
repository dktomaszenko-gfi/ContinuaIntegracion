userName = 'Darek'
userID = 'dktomaszenko'
def mostrarVersionMaven(String a)
{
    bat "java -version"
    echo a
}

pipeline {
    agent any

    stages {
        stage('Info') {
            steps {
                echo "Proceso ejecutado por ${userName}, identificado como ${userID}"
            }
        }
        stage('Java version') {
            steps {
                mostrarVersionMaven("Output")
            }
        }
     
    }
}
